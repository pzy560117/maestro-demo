# 应用配置
PORT=8360
NODE_ENV=development
API_PREFIX=/api/v1

# 数据库配置
DATABASE_URL="postgresql://maestro:maestro123@localhost:5432/maestro?schema=public"

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379

# MinIO 对象存储配置
MINIO_ENABLED=false
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_USE_SSL=false
MINIO_BUCKET=maestro
MINIO_PRESIGNED_EXPIRY_SECONDS=300

# Appium 配置
APPIUM_ENABLED=true
APPIUM_SERVER_URL=http://localhost:4723

# MidScene 视觉解析配置
MIDSCENE_ENABLED=true
MIDSCENE_API_ENDPOINT=http://localhost:8080
MIDSCENE_API_KEY=your-midscene-api-key
MIDSCENE_MAX_CONCURRENCY=3
MIDSCENE_TIMEOUT_MS=8000
MIDSCENE_WARN_LATENCY_MS=2000

# LLM API 配置
LLM_API_ENDPOINT=http://localhost:8000/v1/chat/completions
LLM_API_KEY=your-api-key-here
LLM_MODEL=qwen-vl-max

# CORS 配置
CORS_ORIGIN=http://localhost:5173

# 日志配置
LOG_LEVEL=debug

